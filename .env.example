# Cloudflare Stream Environment Variables (for production)
# Copy this to .env.local and fill in your actual values

# Required for Cloudflare Stream API (if using signed URLs)
# CLOUDFLARE_ACCOUNT_ID=your_cloudflare_account_id
# CLOUDFLARE_STREAM_API_TOKEN=your_stream_api_token

# Optional: Cloudflare Stream Customer Code (if using custom domain)
# CLOUDFLARE_STREAM_CUSTOMER_CODE=your_customer_code

# Production deployment domain (for CORS/CSP)
# VITE_PRODUCTION_DOMAIN=https://yourdomain.com

# Enable production error logging
VITE_ENABLE_VIDEO_LOGGING=true